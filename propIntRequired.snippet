<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Propriété type int obligatoire</Title>
            <Author>jetonpeche</Author>
            <Description>Créer une propriété de type int obligatoire</Description>
            <Shortcut>propir</Shortcut>
        </Header>
        <Snippet>
            <Code Language="CSharp">
                <![CDATA[public required int $NomValeur$ { get; $Setter$; }]]>
            </Code>
            <Declarations>
                <Literal>
                    <ID>NomValeur</ID>
                    <Default>Nom</Default>
                    <ToolTip>Nom de la propriété</ToolTip>
                </Literal>

                <Literal>
                    <ID>Setter</ID>
                    <Default>set</Default>
                </Literal>
            </Declarations>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>